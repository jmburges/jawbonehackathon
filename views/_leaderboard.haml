.row
  .col-md-6
    %h3 Healthy Hotspots
    %ul
      - @locations.take(5).each do |location|
        %li 
          %span{:class=>"label label-default"}
            %img{:src=>"#{location[1]["venue"]["categories"][0]["icon"]}"}
            = location[1]["venue"]["categories"][0]["name"]
          %a{:href=>"#{location[1]["venue"]["url"]}"} 
            %h4 
              = location[1]["venue"]["name"]
              %span.label.label-warning #{location[1]["health_score"]}
  .col-md-6
    %h3 Danger Zones
    %ul
      - @locations.take(5).each do |location|
        %li 
          %span{:class=>"label label-default"}
            %img{:src=>"#{location[1]["venue"]["categories"][0]["icon"]}"}
            = location[1]["venue"]["categories"][0]["name"]
          %a{:href=>"#{location[1]["venue"]["url"]}"} 
            %h4 
              = location[1]["venue"]["name"]
              %span.label.label-warning #{location[1]["health_score"]}